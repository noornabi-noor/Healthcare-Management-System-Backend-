model MedicalReport {
    id         String @id @default(uuid(7))
    reportName String
    reportLink String

    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt

    patientId String
    patient   Patient @relation(fields: [patientId], references: [id], onDelete: Cascade, onUpdate: Cascade)

    @@index([patientId])
    @@map("medical_reports")
}
